# StagedTokenFactory 测试环境配置

# 私钥（用于签名交易）
# 警告：不要在生产环境中使用真实私钥！
# 获取BSC测试网BNB: https://testnet.binance.org/faucet-smart
PRIVATE_KEY=your_private_key_here

# 是否执行实际交易（true/false）
# 设置为 false 只进行模拟测试，不消耗真实BNB
# 设置为 true 会执行真实交易，需要账户有足够的BNB
EXECUTE_TRANSACTIONS=false

# 模拟模式（true/false）
# 设置为 true 进行模拟测试，不发送真实交易
# 设置为 false 允许发送真实交易（需要EXECUTE_TRANSACTIONS=true）
SIMULATION_MODE=true

# RPC端点配置（可选，脚本有默认值）
BSC_TESTNET_RPC=https://bsc-testnet-rpc.publicnode.com

# 测试配置
TEST_MODE=development
VERBOSE_LOGGING=true

# 合约地址（已在脚本中硬编码，此处仅作记录）
FACTORY_CONTRACT_ADDRESS=0x073faD54A73333EC1671522b9cCCbbBd153DA265

# 测试代币配置
TEST_TOKEN_NAME=StagedTestToken
TEST_TOKEN_SYMBOL=STT
TEST_TOKEN_SUPPLY=1000000
TEST_TOKEN_BUY_FEE=1
TEST_TOKEN_SELL_FEE=4
